import java.util.Scanner;

public class drinks {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int sum = 0;

        for (int i = 0; i < n; i++) {
            int drink = sc.nextInt();
            sum = sum + drink;
        }

        double percent = (double) sum / n;
        System.out.println(percent);
        
        sc.close();
    }
}
