import java.util.*;
public class Anton_and_Danik{
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        String s = sc.next();

        int anton_wins =0, danik_wins=0;

        for(int i = 0; i< n; i++){
            if(s.charAt(i) == 'A')  anton_wins++;
            else danik_wins++;
        }

        if(anton_wins > danik_wins)  System.out.println("Anton");
        else if (danik_wins > anton_wins) System.out.println("Danik");
        else System.out.println("Friendship");
    }
}
